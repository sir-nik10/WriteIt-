<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".adminControls.RemoveUserPost">

    <Button
        android:id="@+id/removeUserPostBackButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="26dp"
        android:text="Back"
        app:layout_constraintBottom_toTopOf="@+id/RemoveUserPostTitle"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.091"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/RemoveUserPostTitle"
        android:layout_width="216dp"
        android:layout_height="67dp"
        android:layout_marginBottom="96dp"
        android:gravity="center"
        android:text="Remove User Post!"
        app:layout_constraintBottom_toTopOf="@+id/SearchUserButton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/RemoveUserPostFindUser"
        android:layout_width="199dp"
        android:layout_height="79dp"
        android:layout_marginBottom="32dp"
        android:hint="username"
        android:gravity="center"
        app:layout_constraintBottom_toTopOf="@+id/RemoveUserPostFindPost"
        app:layout_constraintEnd_toStartOf="@+id/SearchUserButton"
        app:layout_constraintHorizontal_bias="0.439"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/RemoveUserPostFindPost"
        android:layout_width="199dp"
        android:layout_height="79dp"
        android:layout_marginStart="28dp"
        android:layout_marginBottom="56dp"
        android:gravity="center"
        android:hint="postId"
        app:layout_constraintBottom_toTopOf="@+id/RemoveUserPostPlayArea"
        app:layout_constraintEnd_toStartOf="@+id/SearchUserButton"
        app:layout_constraintHorizontal_bias="0.025"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/SearchUserButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="52dp"
        android:layout_marginBottom="144dp"
        android:text="Search"
        app:layout_constraintBottom_toTopOf="@+id/RemoveUserPostPlayArea"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/RemoveUserPostPlayArea"
        android:layout_width="263dp"
        android:layout_height="115dp"
        android:layout_marginBottom="56dp"
        android:gravity="center"
        app:layout_constraintBottom_toTopOf="@+id/RemoveUserPostConfirmButton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/RemoveUserPostConfirmButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="84dp"
        android:text="Confirm"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>